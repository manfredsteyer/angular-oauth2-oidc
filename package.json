{
  "name": "starter",
  "version": "10.0.2",
  "license": "MIT",
  "scripts": {
    "release": "standard-version",
    "ng": "ng",
    "start": "ng serve --project sample -o",
    "build": "npm run prettier && ng build --prod --project lib && npm run copy:readme && npm run copy:license && npm run docs",
    "build:jwks": "npm run prettier && ng build angular-oauth2-oidc-jwks --ts-config tsconfig.npm.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tsc": "tsc",
    "prettier": "prettier --write projects/**/*.ts",
    "docs": "npm run docs:build -- --disableCoverage --disablePrivate --disableInternal --includes docs-src",
    "docs:build": "compodoc -p projects/lib/tsconfig.lib.json -n angular-oauth2-oidc -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "format": "prettier --single-quote --write projects/**/*.ts",
    "copy:readme": "cpr README.md dist/lib/README.md --overwrite",
    "copy:license": "cpr LICENSE dist/lib/LICENSE --overwrite",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "contributer": "git shortlog -s 9.0.0..HEAD"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.5",
    "@angular/common": "11.2.5",
    "@angular/compiler": "11.2.5",
    "@angular/core": "11.2.5",
    "@angular/elements": "11.2.5",
    "@angular/forms": "11.2.5",
    "@angular/platform-browser": "11.2.5",
    "@angular/platform-browser-dynamic": "11.2.5",
    "@angular/router": "11.2.5",
    "@webcomponents/custom-elements": "^1.4.3",
    "angular-oauth2-oidc": "^9.2.2",
    "angular-oauth2-oidc-jwks": "^9.0.0",
    "base64-js": "^1.5.1",
    "bootstrap": "^3.3.7",
    "jsrsasign": "^10.1.13",
    "ngx-semantic-version": "^2.3.0",
    "rxjs": "6.5.4",
    "rxjs-compat": "^6.6.6",
    "text-encoder-lite": "^1.0.1",
    "tslib": "^2.1.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.3",
    "@angular/cli": "^11.2.3",
    "@angular/compiler-cli": "11.2.5",
    "@angular/language-service": "11.2.5",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.20.5",
    "codelyzer": "^6.0.0",
    "commitizen": "^4.2.3",
    "conventional-changelog-cli": "^2.1.1",
    "cpr": "^3.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "github-contributors-list": "^1.2.4",
    "husky": "^5.1.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "markdown-magic": "^2.0.0",
    "markdown-magic-github-contributors": "0.0.3",
    "ng-packagr": "^11.2.4",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "standard-version": "^9.1.1",
    "ts-node": "~8.6.2",
    "tslint": "^6.1.3",
    "typescript": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
